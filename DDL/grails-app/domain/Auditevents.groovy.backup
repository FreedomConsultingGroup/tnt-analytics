/**
 * The Auditevents entity.
 *
 * @author    
 *
 *
 */
class Auditevents {
    static mapping = {
         table 'auditevents'
         // version is set to false, because this isn't available by default for legacy databases
         version false
         id column:'id'
    }
    Long id
    String description
    String code
    Date eventDate

    static constraints = {
        id(max: 9999999999L)
        description(size: 1..2000, blank: false)
        code(size: 1..50, blank: false)
        eventDate(nullable: true)
    }
    String toString() {
        return "${id}" 
    }
}
